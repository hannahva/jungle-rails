<section class="user-new">

  <header class="page-header">
    <h1>Login</h1>
  </header>

  <div class="row">
    <div class="col-sm-6 col-sm-offset-1">

      <%= form_tag :login do |f| %>
        <%= f.label :email, "Enter Email" %>
        <%= f.text_field :email %>

        <%= f.label :password, " Enter Password" %>
        <%= f.password_field :password %>
        <%= f.submit "Submit" %>
      <% end %>

    </div>
  </div>
</section>