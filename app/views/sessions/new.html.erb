<section class="user-login">

  <header class="page-header">
    <h1>Login</h1>
  </header>

  <div class="row">
    <div class="col-sm-6 col-sm-offset-1">

      <%= form_tag "/login" do %>

        <%= label_tag :email, "Enter Email" %>
        <%= text_field_tag :email %>

        <%= label_tag :password, " Enter Password" %>
        <%= password_field_tag :password %>
        <%= submit_tag "Submit" %>
      <% end %>
    </div>
  </div>
</section>